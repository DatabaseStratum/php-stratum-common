{
  "name": "setbased/php-stratum-common",
  "description": "PhpStratum Common",
  "keywords": [
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "ext-mbstring": "*",
    "setbased/antlr4-php-runtime": "^0.5.1",
    "setbased/exception": "^2.3.0",
    "setbased/helper-cast": "^3.0.0",
    "setbased/php-stratum-backend": "^5.5.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phing/phing": "^3.0.0-RC4",
    "phpunit/phpunit": "^9.5.28"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Stratum\\Common\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Stratum\\Common\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
