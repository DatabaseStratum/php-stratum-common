{
  "name": "setbased/php-stratum-common",
  "description": "PhpStratum Common",
  "keywords": [
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.4",
    "ext-mbstring": "*",
    "setbased/antlr4-php-runtime": "^0.5.1",
    "setbased/exception": "^2.1.2",
    "setbased/helper-cast": "^2.1.1",
    "setbased/php-stratum-backend": "^5.3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.0",
    "setbased/phing-extensions": "^2.1.0"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Stratum\\Common\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Stratum\\Common\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
