{
  "name": "setbased/php-stratum-common",
  "description": "PhpStratum Common",
  "keywords": [
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "setbased/exception": "^2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "setbased/phing-extensions": "^2.1"
  },
  "autoload": {
    "psr-4": {
